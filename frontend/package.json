{
  "name": "redux-crud-microservice-starterkit",
  "version": "1.0.0",
  "description": "Redux starterkit with full CRUD & microservice capabilities",
  "main": "./app/server/index.js",
  "scripts": {
    "test": "NODE_ENV=testing ./node_modules/.bin/mocha --require ./test/dom.js --require ignore-styles --compilers js:babel-core/register --reporter nyan ./app/src/**/*.spec.js",
    "start-dev": "node app/server/index.js",
    "start-production": "NODE_ENV=production node app/server/index.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack app/src/routes.react.js --config app/build/webpack.config.js"
  },
  "author": "Hedley Smith <mail@hedleysmith.com>",
  "license": "MIT",
