    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.24.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp-autoprefixer": "^3.1.0",
    "history": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "postcss-color-function": "^2.0.1",
